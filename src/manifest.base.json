{
    "name": "ytdpnl",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*", "https://*.youtu.be/*"],
          "js": ["content-script.js"]
        }
      ],
    "browser_specific_settings": {
        "gecko": {
          "update_url": "https://raw.githubusercontent.com/djfm/ytdpnl-extension/main/update_manifest.json"
        }
      }
}
