{
    "name": "Algorithmic Personalization Project",
    "version": "3.0.0.5",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*", "https://*.youtu.be/*"],
          "js": ["content-script.js"],
          "run_at": "document_end"
        },
        {
          "matches": ["https://*.youtube.com/*", "https://*.youtu.be/*"],
          "js": ["loader-masker.js"],
          "run_at": "document_start"
        }
      ],
    "permissions": [
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}
