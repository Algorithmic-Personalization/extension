{
	"name": "YouTube Experiment",
	"version": "1.3.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"https://*.youtube.com/*",
				"https://*.youtu.be/*"
			],
			"js": [
				"content-script.js"
			]
		}
	],
	"permissions": [
		"tabs",
		"*://*.youtube.com/*",
		"*://*.youtu.be/*",
		"*://personalization-server.csail.mit.edu/*"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	}
}